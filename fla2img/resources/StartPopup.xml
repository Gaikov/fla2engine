<?xml version="1.0"?>
<dialog title="Export options" buttons="accept, cancel">
    <hbox>
        <label control="scale" value="Shapes scale:"/>
        <textbox id="scale" value="#{shapes_scale}"/>
    </hbox>

    <label control="imagesGroup" value="Export images:"/>
    <radiogroup id="imagesGroup">
        <radio label="Selected library items" value="selected"/>
        <radio label="All library" value="all"/>
    </radiogroup>
</dialog>

